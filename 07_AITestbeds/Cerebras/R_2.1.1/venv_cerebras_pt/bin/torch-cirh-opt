#!/home/dream_new_world/R_2.1.1/venv_cerebras_pt/bin/python3.8

import os
import subprocess
import sys

import cerebras_pytorch

if __name__ == "__main__":
    start_path = cerebras_pytorch.__path__[0]
    raw_cmd = [os.path.join(start_path, "lib/torch-cirh-opt")]
    raw_cmd.extend(sys.argv[1:])
    subprocess.run(raw_cmd)
