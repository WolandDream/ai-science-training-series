#!/home/dream_new_world/R_2.1.1/venv_cerebras_pt/bin/python3.8

import os
import subprocess
import sys

import cerebras_appliance

if __name__ == "__main__":
    start_path = cerebras_appliance.__path__[0]
    raw_cmd = [os.path.join(start_path, "bin/raw/csctl")]
    raw_cmd.extend(sys.argv[1:])
    subprocess.run(raw_cmd, stdin=sys.stdin, stdout=sys.stdout, stderr=sys.stderr)
